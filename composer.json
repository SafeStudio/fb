  "description": "Firebase REST API easy wrapper for Laravel and Lumen",
  "license": "MIT",
  "keywords": [
    "framework",
    "laravel",
    "lumen",
    "firebase",
    "wrapper",
    "rest",
    "api"
  ],
  "authors": [
    {
      "name": "Le Minh Tri",
      "email": "ansidev@gmail.com"
    }
